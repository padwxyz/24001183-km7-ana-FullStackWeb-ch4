<table class="table table-striped">
    <thead class="text-center">
        <tr>
            <th>Name</th>
            <th>Photo Profile</th>
            <th>Age</th>
            <th>Phone Number</th>
        </tr>
    </thead>

    <tbody class="text-center">
        <% users.map((user)=> { %>
            <tr>
                <td>
                    <%= user.firstName %>
                </td>
                <td>
                    <img src="<%= user.photoProfile %>" alt="Profile photo" class="rounded mx-auto d-block"
                        style="width: 100px; height: 100px;">
                </td>
                <td>
                    <%= user.age %>
                </td>
                <td>
                    <%= user.phoneNumber %>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>